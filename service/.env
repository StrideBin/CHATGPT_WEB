OPENAI_ACCESS_TOKEN=

# OpenAI API Base URL - https://api.openai.com
# OpenAI API的基本URL。默认为“https://api.openai.com”。
OPENAI_API_BASE_URL=

# OpenAI API Model - https://platform.openai.com/docs/models
# 要使用的OpenAI模型的名称。有关可用模型的信息，请参见https://platform.openai.com/docs/models。
OPENAI_API_MODEL=

# set `true` to disable OpenAI API debug log
# 如果设置为“true”，则禁用OpenAI API的调试日志。
OPENAI_API_DISABLE_DEBUG=

# Reverse Proxy - Available on accessToken
# Default: https://ai.fakeopen.com/api/conversation
# More: https://github.com/transitive-bullshit/chatgpt-api#reverse-proxy
# 可用于访问OpenAI API的反向代理的URL。
API_REVERSE_PROXY=

# timeout
# 超时时间（以毫秒为单位）。
TIMEOUT_MS=100000

# Rate Limit
# 允许每小时发送的最大请求数。
MAX_REQUEST_PER_HOUR=

# Secret key
# 授权密钥。
AUTH_SECRET_KEY=

# Socks Proxy Host
# SOCKS代理服务器的主机名或IP地址。
SOCKS_PROXY_HOST=

# Socks Proxy Port
# SOCKS代理服务器的端口号。
SOCKS_PROXY_PORT=

# Socks Proxy Username
# SOCKS代理服务器的用户名（如果需要）。
SOCKS_PROXY_USERNAME=

# Socks Proxy Password
# SOCKS代理服务器的密码（如果需要）。
SOCKS_PROXY_PASSWORD=

# HTTPS PROXY
# 用于访问OpenAI API的HTTPS代理的URL。
HTTPS_PROXY=

